                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT pli.ProductRef,
       [Pli].[Fee],
       isnull([Plr].[SalesOfficeRef],-1) as [SalesOfficeRef],
       [pl].[Number] AS [PriceListNumber],
       CASE
           WHEN [pl].[State] = 1 THEN
              N'ثبت شد'
           WHEN [pl].[State] = 2 THEN
               N'تایید شده'
           WHEN [pl].[State] = 3 THEN
               N'پایان یافته'
       END AS [PriceListState],
       [pl].[StartDate],
       [pl].[EndDate],
	   cast(pli.PriceListItemID as varchar(100)) + '-' + cast(isnull([Plr].[SalesOfficeRef],-1) as varchar(100)) as PriceListAlternateKey1
FROM [SLS3].[PriceListItem] [Pli]
    JOIN [sls3].[PriceList] [pl]
        ON pl.PriceListID = pli.PriceListRef
    LEFT JOIN [SLS3].[PriceListResultRow] [Plr]
        ON [Plr].[PriceListRef] = [Pli].[PriceListRef]</property>
