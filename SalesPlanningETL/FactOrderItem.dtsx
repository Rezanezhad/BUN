       [OI].[Fee],
       [OI].[Price],
       [OI].[ReductionAmount],
       [OI].[AdditionAmount],
       [OI].[NetPrice],
       CAST([OI].[Version] AS BIGINT) AS [Version]
FROM [SLS3].[OrderItem] AS [OI]
    JOIN [SLS3].[Order] AS [O]
        ON [O].[OrderID] = [OI].[OrderRef]
WHERE [OI].[OrderRef] IN
      (
          SELECT [cte].[OrderRef] FROM [cte]
