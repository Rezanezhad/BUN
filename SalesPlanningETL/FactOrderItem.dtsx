                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH [cte]
AS (SELECT DISTINCT
           [OI].[OrderRef]
    FROM [SLS3].[OrderItem] [OI]
    WHERE CAST([OI].[Version] AS BIGINT) &gt; ?)
SELECT [OI].[OrderItemID],
       [OI].[OrderRef],
       (
           SELECT TOP (1)
                  [QI].[QuotationRef]
           FROM [SLS3].[QuotationItem] AS [QI]
           WHERE [QI].[QuotationItemID] = [OI].[ReferenceRef]
