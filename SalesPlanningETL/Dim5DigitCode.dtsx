                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT CG.new_name BookName,
       CG.new_ProductCode,
       GMG.new_name MainGroup,
       new_GradeBase.new_name grade,
       new_SubGradeBase.new_name subgrade,
       new_FieldBase.new_name Field,
       new_LessonBase.new_name Lesson,
       new_BookNameBase.new_name SubLesson,
       new_BranchName BranchName,
       BT.new_MainClassification MainBookType,
       BT.new_name BookType,
       smp.Value group_manager,
       new_azmoongroupBase.new_name AzmoonName,
       new_azmoondateBase.new_name AzmoonDate,
       new_CoverBase.new_name Cover,
       smp3.Value CoverName,
       new_SubstanceSIzeBase.new_name Size,
       new_SubstanceWeightBase.new_name Weight,
       CG.new_ViewOnSite,
       CG.new_SiteBookName,
       smp1.Value Format,
       smp2.Value colornumber,
       CG.new_ispanton IsPanthon,
       new_PanthonCodeBase.new_name PanthonName,
       --smp4.Value consumable,
       CG.new_price Price,
       smp5.Value paperno,
       smp6.Value designtype,
       smp12.Value producttypecontent,
       smp7.Value Covertype,
       smp8.Value Mojeld,
       smp9.Value material,
       smp10.Value papertype,
       smp11.Value contentpapertype,
       smp13.Value bindingtype,
       smp14.Value producttype,
       smp15.Value audience,
       new_maintypeBase.new_name StoryType,
       new_genreBase.new_name genre,
       smp16.Value productiontype,
       (
           SELECT [i].[new_code]
           FROM [dbo].[new_gametechnicalcodesBase] AS [i]
           WHERE [i].[new_gametechnicalcodesId] = CG.[new_danimoontechcode]
       ) AS [gametechnicalcode],
       --new_gridBase.new_name Grid,
       --new_ZinkTypeBase.new_name Zinkname,
       --new_StateBase.new_name,
       CG.statecode,
       CG.CreatedOn
FROM dbo.new_CodeGeneratorBase CG
    JOIN new_Good_Main_GroupBase GMG
        ON GMG.new_Good_Main_GroupId = CG.new_Good_Main_Group
    LEFT JOIN new_GradeBase
        ON new_GradeBase.new_GradeId = CG.new_Grade
    LEFT JOIN new_SubGradeBase
        ON new_SubGradeBase.new_SubGradeId = CG.new_SubGrade
    LEFT JOIN new_LessonBase
        ON new_LessonBase.new_LessonId = CG.new_Lesson
    LEFT JOIN new_FieldBase
        ON new_FieldBase.new_FieldId = CG.new_Field
    LEFT JOIN dbo.new_BookTypeBase BT
        ON BT.new_BookTypeId = CG.new_BookType
    LEFT JOIN dbo.new__Codegenerator_BranchBase GBr
        ON GBr.new_new_codegenerator = CG.new_CodeGeneratorId
    LEFT JOIN dbo.new_BranchBase Br
        ON Br.new_BranchId = GBr.new_new_branch
    --LEFT JOIN dbo.new_Branch_BooktypeBase
    --    ON new_Branch_BooktypeBase.new_new_booktype = new_BookTypeBase.new_BookTypeId
    --LEFT JOIN dbo.new_BranchBase
    --    ON new_BranchBase.new_BranchId = new_Branch_BooktypeBase.new_new_branch
    LEFT JOIN new_azmoongroupBase
        ON new_azmoongroupBase.new_azmoongroupId = CG.new_azmoongroup
    LEFT JOIN new_azmoondateBase
        ON new_azmoondateBase.new_azmoondateId = CG.new_azmoondate
    LEFT JOIN new_CoverBase
        ON new_CoverBase.new_CoverId = CG.new_Cover
    LEFT JOIN new_maintypeBase
        ON new_maintypeBase.new_maintypeId = CG.new_maintype
    LEFT JOIN new_SubstanceSIzeBase
        ON new_SubstanceSIzeBase.new_SubstanceSIzeId = CG.new_Size
    LEFT JOIN new_SubstanceWeightBase
        ON new_SubstanceWeightBase.new_SubstanceWeightId = CG.new_Weight
    LEFT JOIN new_PanthonCodeBase
        ON new_PanthonCodeBase.new_PanthonCodeId = CG.new_PanthonCode
    --LEFT JOIN new_gridBase
    --    ON new_gridBase.new_gridId = CG.new_grid
    --LEFT JOIN new_ZinkTypeBase
    --    ON new_ZinkTypeBase.new_ZinkTypeId = CG.new_ZinkType
    LEFT JOIN new_brandBase
        ON new_brandBase.new_brandId = CG.new_Brand
    --LEFT JOIN new_StateBase
    --    ON new_StateBase.new_StateId = CG.new_State
    LEFT JOIN new_BookNameBase
        ON new_BookNameBase.new_BookNameId = CG.new_BookName
    LEFT JOIN dbo.new_genreBase
        ON new_genreBase.new_genreId = CG.new_genre
    LEFT JOIN dbo.StringMapBase smp
        ON CG.new_group_manager = smp.AttributeValue
           AND smp.AttributeName = 'new_group_manager'
           AND smp.LangId = '1033'
           AND smp.ObjectTypeCode = '1024'
    LEFT JOIN dbo.StringMapBase smp1
        ON CG.new_format = smp1.AttributeValue
           AND smp1.AttributeName = 'new_format'
           AND smp1.LangId = '1033'
           AND smp1.ObjectTypeCode = '10099'
    LEFT JOIN dbo.StringMapBase smp2
        ON CG.new_colornumber = smp2.AttributeValue
           AND smp2.AttributeName = 'new_colornumber'
           AND smp2.LangId = '1033'
           AND smp2.ObjectTypeCode = '1024'
    LEFT JOIN dbo.StringMapBase smp3
        ON CG.new_Cover_name = smp3.AttributeValue
           AND smp3.AttributeName = 'new_cover_name'
           AND smp3.LangId = '1033'
           AND smp3.ObjectTypeCode = '10067'
    --LEFT JOIN dbo.StringMapBase smp4
    --    ON CG.new_ConsumableCode = smp4.AttributeValue
    --       AND smp4.AttributeName = 'new_consumablecode'
    --       AND smp4.LangId = '1033'
    --       AND smp4.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp5
        ON CG.new_paperno = smp5.AttributeValue
           AND smp5.AttributeName = 'new_paperno'
           AND smp5.LangId = '1033'
           AND smp5.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp6
        ON CG.new_designtype = smp6.AttributeValue
           AND smp6.AttributeName = 'new_designtype'
           AND smp6.LangId = '1033'
           AND smp6.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp7
        ON CG.new_covertype = smp7.AttributeValue
           AND smp7.AttributeName = 'new_covertype'
           AND smp7.LangId = '1033'
           AND smp7.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp8
        ON CG.new_mojeld = smp8.AttributeValue
           AND smp8.AttributeName = 'new_mojeld'
           AND smp8.LangId = '1033'
           AND smp8.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp9
        ON CG.new_material = smp9.AttributeValue
           AND smp9.AttributeName = 'new_material'
           AND smp9.LangId = '1033'
           AND smp9.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp10
        ON CG.new_PaperType = smp10.AttributeValue
           AND smp10.AttributeName = 'new_papertype'
           AND smp10.LangId = '1033'
           AND smp10.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp11
        ON CG.new_contentpapertype = smp11.AttributeValue
           AND smp11.AttributeName = 'new_contentpapertype'
           AND smp11.LangId = '1033'
           AND smp11.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp12
        ON CG.new_ProductTypeContent = smp12.AttributeValue
           AND smp12.AttributeName = 'new_producttypecontent'
           AND smp12.LangId = '1033'
           AND smp12.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp13
        ON CG.new_bindingtype = smp13.AttributeValue
           AND smp13.AttributeName = 'new_bindingtype'
           AND smp13.LangId = '1033'
           AND smp13.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp14
        ON CG.new_producttype = smp14.AttributeValue
           AND smp14.AttributeName = 'new_producttype'
           AND smp14.LangId = '1033'
           AND smp14.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp15
        ON CG.new_audience = smp15.AttributeValue
           AND smp15.AttributeName = 'new_audience'
           AND smp15.LangId = '1033'
           AND smp15.ObjectTypeCode = '10067'
    LEFT JOIN dbo.StringMapBase smp16
        ON CG.new_productiontype = smp16.AttributeValue
           AND smp16.AttributeName = 'new_productiontype'
           AND smp16.LangId = '1033'
           AND smp16.ObjectTypeCode = '10067'
WHERE CG.new_ProductCode IS NOT NULL</property>
