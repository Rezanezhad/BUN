                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT top(1000) * FROM (SELECT  cast(v.[Date] as datetime) AS date, ag.Code as General_Code ,ag.Title AS General_Title,
  g.Code AS Group_Code, g.Title AS Group_Title,
  s.Code AS Ledger_Code,s.Title AS Ledger_Title,
  isnull(cast( 
	b.BranchID as int) ,10) as Organization_Id,

  b.Title as Organization_Title,
  v.VoucherTypeRef, v.Number,vi.VoucherItemID,VI.[Description],SUM(vi.Credit) AS credit , SUM(vi.Debit) AS debit,DL4.Title AS 'DL4',DL4.Code as 'dl4cod',
  DL5.Title AS 'DL5',DL5.Code as'dl5cod' ,DL6.Title AS 'DL6',dl6.Code AS 'dl6cod',dl4.DLTypeRef  AS 'dltyperef4',dl5.DLTypeRef AS 'dltyperef5',dl6.DLTypeRef AS 'dltyperef6'

  FROM FIN3.VoucherItem vi
  LEFT JOIN FIN3.Voucher v ON vi.VoucherRef=v.VoucherID
  LEFT JOIN FIN3.SL s ON vi.SLRef=s.SLID
  LEFT JOIN FIN3.GL g ON vi.GLRef=g.GLID
  LEFT JOIN GNR3.Branch b ON vi.BranchRef=b.BranchID
  --LEFT JOIN DimDate dd ON CAST(v.Date AS DATE)=CAST(dd.Date AS DATE)
  LEFT JOIN FIN3.AccountGroup ag ON vi.AccountGroupRef=ag.AccountGroupID
  FULL JOIN (SELECT DISTINCT VI1.VoucherItemID AS MM,D.Title,d.DLTypeRef,D.Code

    FROM FIN3.VoucherItem vi1 LEFT JOIN FIN3.DL d ON vi1.DLLevel4=d.Code) AS DL4 ON DL4.MM=VI.VoucherItemID
 
  FULL JOIN (SELECT DISTINCT VI2.VoucherItemID AS MM1 ,D1.Title,d1.DLTypeRef,D1.Code

    FROM FIN3.VoucherItem vi2 LEFT JOIN FIN3.DL d1 ON vi2.DLLevel5=d1.Code) AS DL5 ON DL5.MM1=VI.VoucherItemID

  FULL JOIN (SELECT DISTINCT VI3.VoucherItemID AS MM2 ,D2.Title,d2.Code,d2.DLTypeRef

    FROM FIN3.VoucherItem vi3 LEFT JOIN FIN3.DL d2 ON vi3.DLLevel6=d2.Code) AS DL6 ON DL6.MM2=VI.VoucherItemID




  WHERE  v.State &lt;&gt; 2
  --AND s.Code IN (305001, 305008, 402001, 402002, 402003, 205010, 205011, 806003, 305003, 305009, 806001, 806002, 806009, 109011)
  --AND LEFT(dd.PersianDateKey,4)&gt;=1399

  GROUP BY v.Date ,s.Code,s.Title,dl4.DLTypeRef,dl5.DLTypeRef,dl6.DLTypeRef,dl6.Code,g.Code ,b.BranchID,b.Title,g.Title,ag.Code,ag.Title,v.VoucherTypeRef,v.Number,vi.VoucherItemID,VI.[Description],DL4.Title,DL4.Code,DL5.Title,DL5.Code,DL6.Title,DL6.Code) AS sff</property>
